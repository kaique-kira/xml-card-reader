{
  "name": "xml-card-reader",
  "version": "1.0.1",
  "description": "Biblioteca NPM para leitura e parse de XML com TypeScript",
  "keywords": [
    "xml",
    "emv",
    "apdu",
    "parser",
    "card"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaique-kira/xml-card-reader.git"
  },
  "bugs": {
    "url": "https://github.com/kaique-kira/xml-card-reader/issues"
  },
  "homepage": "https://github.com/kaique-kira/xml-card-reader#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "npm run build && npm test",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev:postman": "tsx scripts/postman-server.ts",
    "parse:file": "tsx scripts/parse-xml-cli.ts --file",
    "parse:stdin": "tsx scripts/parse-xml-cli.ts",
    "pack:dry": "npm pack --dry-run",
    "release:check": "npm run check && npm run pack:dry",
    "prepublishOnly": "npm run build && npm run test"
  },
  "author": "Kaique-Felipe",
  "license": "ISC",
  "dependencies": {
    "fast-xml-parser": "^5.2.5",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.13.4",
    "express": "^5.1.0",
    "tsx": "^4.20.4",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  }
}
